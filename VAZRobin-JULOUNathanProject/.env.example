# Clé API OpenAI (optionnel - requis pour l'analyse de code)
OPENAI_API_KEY=sk-...

# Modèle OpenAI à utiliser (par défaut: gpt-4o)
OPENAI_MODEL=gpt-4o

# Clé API Anthropic (optionnel - support Claude)
ANTHROPIC_API_KEY=sk-ant-...

# Configuration Git (optionnel)
GIT_REPO_PATH=.

# Configuration de sortie
OUTPUT_FORMAT=markdown
OUTPUT_LANGUAGE=fr

# Options d'analyse
CHECK_SECURITY=true
CHECK_PERFORMANCE=true
CHECK_STYLE=true